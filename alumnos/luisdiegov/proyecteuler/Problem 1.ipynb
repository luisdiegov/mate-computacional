      "def multiplesSum(a,b,num):\n",
      "    sum=0\n",
      "    i=1\n",
      "    \n",
      "    while i<num:\n",
      "        if i%a==0 or i%b==0:\n",
      "            sum+=i\n",
      "        i+=1\n",
      "    \n",
      "    return sum"
